<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ase_project_nutrismart">

    <application
        android:allowBackup="true"
        android:icon="@drawable/nutrismart"
        android:label="@string/app_name"

        android:supportsRtl="true"
        android:theme="@style/Theme.ASE_Project_NutriSmart">
        <activity
            android:name=".SplashActivity"
            android:exported="true">
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
        </activity>
        <activity
            android:name=".HomeActvity"
            android:exported="false"
            android:label="@string/title_activity_home_actvity"
            android:theme="@style/Theme.ASE_Project_NutriSmart.NoActionBar" />
        <activity
            android:name=".SignupActivity"
            android:exported="false" />
        <activity
            android:name=".LoginActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">

        </activity>
    </application>

</manifest>